import java.util.*;
public class Main
{
    public static int calSum(){
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=a+b;
        return c;
    }
	public static void main(String[] args) {
	    int d=calSum();
	    System.out.print(d);
	}
}
