import java.util.*;
public class Main
{
    public static int calSum(){
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int fact=1;
        for(int i=1; i<=a; i++){
            fact*=i;
        }
        return fact;
    }
	public static void main(String[] args) {
	    int d=calSum();
	    System.out.print(d);
	}
}
