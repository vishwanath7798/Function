import java.util.*;
public class Main
{
    public static boolean prime(){
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        boolean isPrime=true;
        for (int i=2; i<a; i++){
            if(a%i==0){
                return false;
            }
        }
        return true;
    }
	public static void main(String[] args) {
	    System.out.print(prime());
	}
}
